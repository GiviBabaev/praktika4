<Window x:Class="proekt4.Window4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:proekt4"
        mc:Ignorable="d"
        Title="Window4" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Content="Решить" HorizontalAlignment="Left" Margin="125,29,0,0" Grid.Row="2" VerticalAlignment="Top" Width="136" Height="54" Click="Button_Click_5"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="41" Width="323" FontSize="18"><Run Text="Введите процент "/><Run Text="увеличения "/><Run Text="пробега"/></TextBlock>
        <TextBox x:Name="vod" Grid.Column="1" HorizontalAlignment="Left" Height="26" Margin="19,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="127"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="41" Width="323" FontSize="18"><Run Text="Ввведите "/><Run Text="длину пробега в первы день"/></TextBlock>
        <TextBox x:Name="vod2" Grid.Column="1" HorizontalAlignment="Left" Height="26" Margin="19,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="127"/>
        <TextBlock x:Name="otv" Grid.Column="1" HorizontalAlignment="Left" Margin="52,19,0,0" Grid.Row="2" TextWrapping="Wrap" VerticalAlignment="Top" Height="64" Width="269" FontSize="14"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,97,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="41" Width="323" FontSize="18" Text="Введите суммарны пробег"/>
        <TextBox x:Name="vod3" Grid.Column="1" HorizontalAlignment="Left" Height="26" Margin="19,103,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="127"/>
    </Grid>
</Window>
